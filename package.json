{
  "name": "classroom",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-brands-svg-icons": "5.6.3",
    "@fortawesome/free-regular-svg-icons": "5.6.3",
    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@fortawesome/pro-light-svg-icons": "5.6.3",
    "@fortawesome/pro-regular-svg-icons": "5.6.3",
    "@fortawesome/pro-solid-svg-icons": "5.6.3",
    "@fortawesome/react-fontawesome": "0.1.4",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "query-string": "6.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-markdown": "4.0.6",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettify": "prettier --write 'src/**/*.{js,jsx,json,css,scss}'",
    "start-storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o build-storybook"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.1.9",
    "@storybook/addon-centered": "4.1.9",
    "@storybook/addon-info": "4.1.9",
    "@storybook/addon-knobs": "4.1.9",
    "@storybook/addon-links": "4.1.9",
    "@storybook/react": "4.1.9",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "prettier": "1.16.1",
    "source-map-explorer": "1.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11",
      "Android >= 4"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
